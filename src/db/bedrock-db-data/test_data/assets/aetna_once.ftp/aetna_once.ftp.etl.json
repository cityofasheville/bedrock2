{
    "run_group": "aetna_once",
    "tasks": [
      {
        "type": "table_copy",
        "description": "Copy Aetna to S3",
        "source_location": {
          "connection": "munis/munprod/fme_jobs",
          "schemaname": "avl",
          "tablename": "PR_AETNA_Feed_With_History"
        },
        "target_location": {
          "connection": "s3_data_files",
          "filename": "aetna_asheville_WITH_HISTORY_${YYYY}${MM}${DD}.csv",
          "s3_path": "aetna/"
        },
        "active": true
      },
      {
        "type": "file_copy",
        "description": "Copy Aetna S3 to FTP site",
        "source_location": {
          "connection": "s3_data_files",
          "filename": "aetna_asheville_WITH_HISTORY_${YYYY}${MM}${DD}.csv",
          "path": "aetna/"
        },
        "target_location": {
          "connection": "aetna_ftp",
          "filename": "aetna_asheville_WITH_HISTORY_${YYYY}${MM}${DD}.csv",
          "path": "/AetnaSRC_Reports/toAetna/"
        },
        "active": false
      }
    ]
}
