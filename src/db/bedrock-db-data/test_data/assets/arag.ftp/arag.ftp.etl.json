{
  "run_group": "arag",
  "tasks": [
    {
      "type": "table_copy",
      "description": "Copy Arag to S3",
      "source_location": {
        "connection": "munis/munprod/fme_jobs",
        "schemaname": "avl",
        "tablename": "PR_ARAG_Feed",
        "sortdesc": "DATA",
        "fixedwidth_noquotes": true
      },
      "target_location": {
        "connection": "s3_data_files",
        "filename": "arag_asheville_${YYYY}${MM}${DD}.csv",
        "s3_path": "arag/"
      },
      "active": true
    },
    {
      "type": "encrypt",
      "s3_connection": "s3_data_files",
      "s3_path": "arag/",
      "encrypt_connection": "arag_ftp",
      "filename": "arag_asheville_${YYYY}${MM}${DD}.csv",
      "encrypted_filename": "arag_asheville_${YYYY}${MM}${DD}.csv.pgp",
      "active": true
    },
    {
      "type": "file_copy",
      "description": "Copy Arag S3 to FTP site",
      "source_location": {
        "connection": "s3_data_files",
        "filename": "arag_asheville_${YYYY}${MM}${DD}.csv.pgp",
        "path": "arag/"
      },
      "target_location": {
        "connection": "arag_ftp",
        "filename": "arag_asheville_${YYYY}${MM}${DD}.csv.pgp",
        "path": "/"
      },
      "active": true
    }
  ]
}